{
  "name": "deja-cloud",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "run-p type-check build-only",
    "build-only": "vite build",
    "dev": "vite --host --config vite.dev.config.ts",
    "dev:proxy": "vite --host --config vite.dev.config.ts --mode development",
    "dev:mock": "vite --host --config vite.dev.config.ts --mode mock",
    "format": "prettier --write src/",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "preview": "vite preview",
    "start": "vite --host",
    "type-check": "vue-tsc --noEmit --composite false --skipLibCheck "
  },
  "dependencies": {
    "@vercel/blob": "^1.1.1",
    "@vercel/node": "^5.3.12",
    "dayjs": "^1.11.13",
    "firebase": "^10.14.1",
    "vue": "^3.5.17",
    "vue-router": "^4.2.2",
    "vuefire": "^3.2.1",
    "vuetify": "^3.7.3"
  },
  "devDependencies": {
    "@mdi/font": "^7.4.47",
    "@repo/auth": "workspace:^0.0.0",
    "@repo/dccex": "workspace:*",
    "@repo/deja": "workspace:*",
    "@repo/eslint-config": "workspace:*",
    "@repo/firebase-config": "workspace:*",
    "@repo/modules": "workspace:*",
    "@repo/prettier-config": "workspace:*",
    "@repo/sounds": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@repo/ui": "workspace:*",
    "@repo/utils": "workspace:*",
    "@vitejs/plugin-vue": "^4.4.0",
    "@vue/tsconfig": "^0.7.0",
    "@formkit/auto-animate": "^0.8.2",
    "@vueuse/core": "^11.0.3",
    "@vueuse/motion": "^2.2.6",
    "@vueuse/sound": "^2.0.1",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.41",
    "tailwindcss": "^3.4.10",
    "typescript": "*",
    "vite": "^4.4.9",
    "vue-tsc": "^2.2.12",
    "vue3-icons": "^0.1.13",
    "vuedraggable": "^4.1.0"
  }
}
