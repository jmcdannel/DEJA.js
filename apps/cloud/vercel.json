{
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ],
  "functions": {
    "api/bbc-sounds/import.ts": {
      "maxDuration": 30
    }
  },
  "env": {
    "BLOB_READ_WRITE_TOKEN": "@blob-read-write-token"
  }
}